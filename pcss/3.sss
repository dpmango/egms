// LOGIN
.login-bg
  background: url('../img/loginBG.png') no-repeat center
  background-size: cover
  position: relative
  min-height: 100vh
  min-width: 1200px
  display: -webkit-flex
  display: -ms-flex
  display: flex
  flex-direction: column
  @media(height <= 650px)
    min-height: 650px
  .header
    background: transparent
    box-shadow: none
    border: none
    .logo
      background: url('../img/logo-dash.png') no-repeat center
      width: 165px
      height: 34px
      background-size: contain
      margin-top: 8px
  .container-2
    margin-top: 20vh

  .main
    flex: 1


.login
  &-info
    font-size: 16px
    padding-left: 40px
    padding-right: 30px
    line-height: 24px
    color: color(black a(60%))
  .fullwidth
    margin-bottom: 20px

    &.valid-error
      input
        border: 2px solid #cc1b30
    label
      font-size: 16px
      font-family: 'robotoregular'
    label span
      text-transform: none
      float: right
    input
      font-size: 24px
      height: 44px
      line-height: 44px
  &-cta
    &-forgot
      font-size: 16px
      text-decoration: underline
      margin-left: 20px
      font-family: 'robotoregular'

.validation-message
  background: #cc1b30
  color: white
  padding: 9px 10px
  border-radius: 3px
  font-size: 16px
  margin-top: 5px
  font-family: 'robotoregular'
  a
    text-decoration: underline
    float: right
    color: white
    transition: all .3s ease-in-out
    font-family: 'robotoregular'
    &:hover
      opacity: .7

.footer
  flex: 0 0 50px
  position: absolute
  bottom: 10px
  left: 0
  line-height: 1.4em
  width: calc(100% - 60px)
  left: 0
  margin-left: 25px
  margin-right: 25px
  color: color(black a(60%))
  box-sizing: border-box
  max-width: 100%
  &-copy
    float: left
  &-time
    text-align: right
    padding-right: 00px


// DASHBOARD
.container-900
  width: 900px
  margin-left: auto
  margin-right: auto
  padding-left: 30px
  padding-right: 30px
.container-960
  width: 960px
  margin-left: auto
  margin-right: auto
  padding-left: 30px
  padding-right: 30px
.row
  .col-1-3
    width: 33%
  .col-2-3
    width: 66%

.dashboard-bg
  background: #dadde5
  &--light
    background: #eef0f5
    .dashboard-right
      padding-left: 1px
  .main
    display: flex

  .header
    margin-bottom: 0
    .logo
      background: url('../img/logo-dash.png') no-repeat center
      width: 165px
      height: 34px
      background-size: contain
      margin-top: 8px
  .form-control input
    background: white
    &:focus
      background: white

.dashboard-left
  background: #666f80
  float: left
  min-height: calc(100vh - 72px)
  width: 300px
  color: white
  transition: all .5s ease
  flex: 0 1 300px
  position: relative
  &::after
    position: absolute
    top: 0
    right: 0
    z-index: 9
    content: ' '
    width: 100%
    height: 100%
    pointer-events: none
    display: block
    box-shadow: inset -30px 0px 30px -30px rgba(0,0,0,0.4)
  &.collapsed
    width: 50px
    flex: 0 0 50px
    .collapse
      .ico
        transform: rotate(180deg)
    a
      max-width: 52px
      padding-left: 10px
      background: #525966 url('../img/dash-left-carret-small.svg') no-repeat 95% 95%
      span
        display: none
  a
    color: white
  ul
    margin-top: 10px
    margin-bottom: 50px
    li
      display: block
      max-height: 54px
      overflow: hidden
      a
        padding: 14px 20px 14px 17px
        background: #525966 url('../img/dash-left-carret.png') no-repeat 95% center
        border-bottom: 1px solid #7d8493
        border-top: 1px solid #7d8493
        margin-top: -1px
        display: block
        color: white
        transition: all .4s ease-in-out
        font-family: 'oswaldregular'
        text-transform: uppercase
        font-size: 16px
        white-space: nowrap
        span
          vertical-align: middle
        &:hover
          background-color: #3f454f
          text-decoration: none
      &.no-carret
        a
          background: #525966
          &:hover
            background-color: #3f454f
      &.collapse
        a
          background: #7e899e
          &:hover
            background: color(#7e899e a(70%))

  &-group
    margin-top: 10px
    &:first-child
      margin-top: 5px
      margin-bottom: 10px

.dashboard-right-head
  font-size: 30px
  margin-top: 30px
  font-family: 'oswaldregular'
  .flr
    margin-top: -8px

.files
  .ico-close
    size: 38px
    background: url('../img/ico-close.png') no-repeat center
